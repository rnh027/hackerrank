#include <stdio.h>
void main() 
{
	char a[100];
	int count =0,i = 0;
	scanf("%s",a);
	while(a[i])
	{
	    if(a[i]<='9' && a[i]>='0')
	    {
	        count = count*10 +a[i]-'0';
	    }
	    else
	    {
	        while(count--)
	         printf("%c",a[i]);
	        count = 0;
	    }
       i++;
	}
}
*//input : 3A4B3C ouput:AAABBBBCCC
