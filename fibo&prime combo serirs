#include <stdio.h>
#include<math.h>
int prime(int n)
{
    int c = 0,val = 4;
    if(n==1)return 2;
    if(n==2)return 3;
    for(c = 2;c<n;val++)
    {
        if(val%2)
        {
            int sqval = (int)sqrt(val);
            int flag = 0;
            for(int f = 3;f<=sqval;f+=2)
            {
                if(val%f == 0)
                {
                    flag =1;
                    break;
                }
            }
            if(flag==0) c++;
        }
    }
    return val-1;
}
int fibo(int num)
{
    if(num<=2) return 1;
    return fibo(num-1) + fibo(num-2);
}

int main() {
	int n;
	scanf("%d",&n);
	printf("%d",n%2?fibo((n+1)/2):prime(n/2));
	return 0;
}
