def cal(st,i):
    if(ord(st[i]>=65 and st[i]<=91):
        val1 = ord(st[0])-64
        if(val1>10):
            val1 = (val1%10)+val1//10)
    else:
        val1 = 0
    return val1
st = input().upper()
di = {1:2,2:4,3:8,4:16,5:32,6:64,7:128,8:216,9:512,10:1024}
l = len(st)
s = 0
s1 = 0
for key,value in di.items():
    if l<value:
        s = key
        s1 = value
        break
#print(ord(st[0])-64,0,ord(st[s1-s])-64,ord(st[s-1])-64)
v1 = cal(st,0)
v2 = cal(st,s1-1)
v3 = cal()

    
   
    
        
